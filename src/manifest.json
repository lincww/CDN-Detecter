{
  "manifest_version": 3,
  "name": "CDN detector",
  "version": "0.0.0.1",
  "description": "Detect which CDN provider the website uses",
  "icons": {
    "512": "icon.png"
  },
  "action": {
    "default_icon": {
      "512": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.ts",
    "type": "module"
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
